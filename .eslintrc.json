{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {        
        "@typescript-eslint/no-explicit-any": 1,
        "no-unused-vars": 1
    },
    "globals": {
        "console": "writable"
    }
}
